  <default>
    <default class="visual">
      <geom contype="0" conaffinity="0" group="2" rgba="0.75 0.75 0.75 1"/>
    </default>
    <default class="collision">
      <geom group="3"/>
    </default>
    <default class="motor_visual">
      <geom contype="0" conaffinity="0" group="2" rgba="1 1 1 1"/>
    </default>
    <default class="link_visual">
      <geom contype="0" conaffinity="0" group="2" rgba="1 0 1 1"/>
    </default>
    <joint actuatorfrcrange="-50 50"/>
  </default>

  <worldbody>
    <body name="link1">
      <inertial pos="0 0 0" quat="0 0.707107 0 0.707107" mass="1.01" diaginertia="0.001259 0.000851 0.000843"/>
      <joint name="joint_1" pos="0 0 0" axis="0 0 1"/>
      <site name="joint_1_site" pos="0 0 0"/>
      <!-- Motor visual and collision -->
      <geom class="motor_visual" type="cylinder" size="0.05 0.025" pos="0 0 0"/>
      <geom class="collision" type="cylinder" size="0.05 0.025" pos="0 0 0"/>
      <!-- Link visual and collision -->
      <geom class="link_visual" type="cylinder" size="0.025 0.05" pos="0 0 0.05"/>
      <geom class="collision" type="cylinder" size="0.025 0.05" pos="0 0 0.05"/>
      <body name="link2" pos="0 0 0.1" quat="0.707105 0.707108 0 0">
        <inertial pos="0 0 0" quat="0 0.707107 0 0.707107" mass="1.01" diaginertia="0.001259 0.000851 0.000843"/>
        <joint name="joint_2" pos="0 0 0" axis="0 0 1"/>
        <site name="joint_2_site" pos="0 0 0"/>
        <!-- Motor visual and collision -->
        <geom class="motor_visual" type="cylinder" size="0.05 0.025" pos="0 0 0"/>
        <geom class="collision" type="cylinder" size="0.05 0.025" pos="0 0 0"/>
        <!-- Link visual and collision -->
        <geom class="link_visual" type="cylinder" size="0.025 0.05" pos="0 0 0.05"/>
