    <position name="actuator_2" joint="joint_2" kp="1000" kv="100" ctrlrange="-1.5708 1.5708"/>
    <position name="actuator_3" joint="joint_3" kp="1000" kv="100" ctrlrange="-1.5708 1.5708"/>
