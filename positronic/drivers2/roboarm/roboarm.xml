          <geom class="motor_visual" type="cylinder" size="0.05 0.025" pos="0 0 0"/>
          <geom class="collision" type="cylinder" size="0.05 0.025" pos="0 0 0"/>
          <!-- Link visual and collision -->
          <geom class="link_visual" type="cylinder" size="0.025 0.1" pos="0 0 0.1"/>
          <geom class="collision" type="cylinder" size="0.025 0.1" pos="0 0 0.1"/>
          <body name="link4" pos="0 0 0.25" quat="0.707105 0.707108 0 0">
